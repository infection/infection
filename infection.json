{
    "$schema": "./resources/schema.json",
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "stryker": {
            "badge": "master"
        }
    },
    "mutators": {
        "global-ignoreSourceCodeByRegex": [
            "Assert::.*"
        ],
        "@default": true
    }
}
