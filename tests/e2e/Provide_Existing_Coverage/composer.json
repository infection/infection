{
    "config": {
        "platform": {
            "php": "7.2.9"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "phpspec/phpspec": "^4.3 || ^5.1 || ^6.1",
        "leanphp/phpspec-code-coverage": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "spec\\": "spec/",
            "ProvideExistingCoverage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProvideExistingCoverage\\Test\\": "tests/"
        }
    }
}
