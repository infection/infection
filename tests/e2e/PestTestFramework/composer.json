{
    "require-dev": {
        "pestphp/pest": "^1.15.0"
    },
    "autoload": {
        "psr-4": {
            "PestTestFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PestTestFramework\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "conflict": {
        "nikic/php-parser": ">=5.0.0"
    }
}
