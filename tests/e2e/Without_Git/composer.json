{
    "require-dev": {
        "phpunit/phpunit": "^9.5.5",
        "infection/infection": "*"
    },
    "autoload": {
        "psr-4": {
            "Without_Git\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Without_Git\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
