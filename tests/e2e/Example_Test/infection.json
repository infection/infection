{
    "$schema": "../../../resources/schema.json",
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "summary": "infection.log"
    },
    "tmpDir": ".",
    "mutators": {
        "@default": false,
        "ProtectedVisibility": true,
        "Plus": true,
        "global-ignoreSourceCodeByRegex": [
            "public(.*)"
        ],
        "PublicVisibility": true
    }
}
