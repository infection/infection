{
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "phpspec/phpspec": "^4.3",
        "leanphp/phpspec-code-coverage": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "spec\\": "spec/",
            "Namespace_\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Namespace_\\Test\\": "tests/"
        }
    }
}
