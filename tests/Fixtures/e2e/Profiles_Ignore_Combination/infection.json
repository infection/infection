{
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "debug": "infection-log.txt"
    },
    "mutators": {
        "@function_signature": {
            "ignore": [
                "*\\SourceClass*"
            ]
        },
        "FunctionSignature-ProtectedVisibility": false,
        "Boolean-FalseValue": {
            "ignore": [
                "**::isFalse"
            ]
        },
        "Boolean-TrueValue": true
    }
}
