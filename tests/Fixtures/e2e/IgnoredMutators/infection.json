{
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "summary": "infection-log.txt"
    },
    "mutators": {
        "FunctionSignature-PublicVisibility": {
            "ignore": [
                "*\\SourceClass*",
                "*\\OtherClass::hello"
            ]
        },
        "Boolean-FalseValue": {
            "ignore": [
                "**::isFalse"
            ]
        }
    }
}
