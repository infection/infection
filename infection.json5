{
    "$schema": "./resources/schema.json",
    "timeout": 25,
    "source": {
        "directories": [
            "src"
        ],
        excludes: [
            "src/FileSystem/DummyFileSystem.php",
            "src/FileSystem/DummySymfony5FileSystem.php",
            "src/FileSystem/DummySymfony6FileSystem.php"
        ]
    },
    "logs": {
        "stryker": {
            "report": "master"
        }
    },
    "mutators": {
        "global-ignoreSourceCodeByRegex": [
            "Assert::.*"
        ],
        "@default": true
    }
}
